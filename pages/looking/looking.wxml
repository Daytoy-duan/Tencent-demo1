<!--pages/looking/looking.wxml-->
<view class="page">
	<view class="weui-grids">
		<navigator class="weui-grid" bindtap="doSearch" data-keyword="美食" aria-role="button" url="javascript:">
			<view class="weui-grid__icon">
				<image src="{{icon}}" alt></image>
			</view>
			<view class="weui-grid__label">美食</view>
		</navigator>
		<navigator class="weui-grid" bindtap="doSearch" data-keyword="酒店"  aria-role="button" url="javascript:">
			<view class="weui-grid__icon">
				<image src="{{icon}}" alt></image>
			</view>
			<view class="weui-grid__label">酒店</view>
		</navigator>
		<navigator class="weui-grid" bindtap="doSearch" data-keyword="加油站"  aria-role="button" url="javascript:">
			<view class="weui-grid__icon">
				<image src="{{icon}}" alt></image>
			</view>
			<view class="weui-grid__label">加油站</view>
		</navigator>
		<navigator class="weui-grid" bindtap="doSearch" data-keyword="银行"  aria-role="button" url="javascript:">
			<view class="weui-grid__icon">
				<image src="{{icon}}" alt></image>
			</view>
			<view class="weui-grid__label">银行</view>
		</navigator>
		<navigator class="weui-grid" bindtap="doSearch" data-keyword="地铁"  aria-role="button" url="javascript:">
			<view class="weui-grid__icon">
				<image src="{{icon}}" alt></image>
			</view>
			<view class="weui-grid__label">地铁</view>
		</navigator>
		<a class="weui-grid" bindtap="doSearch" data-keyword="公交">
			<view class="weui-grid__icon">
				<image src="{{icon}}" alt></image>
			</view>
			<view class="weui-grid__label">公交</view>
		</a>
    <a class="weui-grid" bindtap="doSearch" data-keyword="医院">
			<view class="weui-grid__icon">
				<image src="{{icon}}" alt></image>
			</view>
			<view class="weui-grid__label">医院</view>
		</a>
		<a class="weui-grid" bindtap="doSearch" data-keyword="公园">
			<view class="weui-grid__icon">
				<image src="{{icon}}" alt></image>
			</view>
			<view class="weui-grid__label">公园</view>
		</a>
	</view>
</view>
